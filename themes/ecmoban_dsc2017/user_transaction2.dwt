	function new_addBonus(){
		var bns = new Object;
		
		bns.bonus_sn = $(".txt_input_cardnum").val();
		bns.password = $(".txt_input_cardpw").val();
		bns.captcha = $(":input[name='captcha']").val();
		
		var foot = true;
		var ok_title, cl_title, divId, title, width, height, hrefCont;
		title = follow_Prompt;
		width = 445;
		height = 58;
		ok_title = determine;
		cl_title = cancel;
		
		var divClass = 'warn-icon-wen';
		if(bns.bonus_sn == ''){
			message = card_number_null;
			divClass = 'warn-icon';
			foot = false;
		}else if(bns.password == ''){
			message = Real_name_password_null;
			divClass = 'warn-icon';
			foot = false;
		}else if(bns.captcha == ''){
			message = null_captcha_login;
			divClass = 'warn-icon';
			foot = false;
		}
		
		divId = "user_add_bonus"; 
		
		if(bns.bonus_sn != '' && bns.password != '' && bns.captcha != ''){
			Ajax.call('user.php', 'act=act_add_bonus&bns=' + $.toJSON(bns), function(data){
				if(data.error == 2){
					hrefCont = "user.php";
					divClass = 'warn-icon-wen';
					ok_title = go_login;
					
				}else if(data.error == 3){
					hrefCont = "";
					divClass = 'warn-icon';
					foot = false;
				}else if(data.error == 0){
					hrefCont = "user.php?act=bonus";
					divClass = 'warn-icon-ok';
				}
				
				var content = '<div id="user_add_bonus">' + 
									'<div class="tip-box icon-box">' +
										'<span class="' +divClass+ ' m-icon"></span>' + 
										'<div class="item-fore">' +
											'<h3 class="ftx-04">' + data.message + '</h3>' +
										'</div>' +
									'</div>' +
								'</div>';
				
				pb({
					id:divId,
					title:title,
					width:width,
					height:height,
					ok_title:ok_title,  //按钮名称
					cl_title:cl_title,  //按钮名称
					content:content,    //调取内容
					drag:false,
					foot:foot,
					onOk:function(){
						if(hrefCont){
							location.href = hrefCont;
						}
					}
				});
				
				$('#' + divId + ' .ftx-04').css({'padding': '11px 0px 0px 10px'});
				$('#' + divId + ' .tip-box').css({
					'width': '330px',
					'height': '50px',
					'padding': '0px 0px 10px 0px'
				});
				
				$('#' + divId + ' .item-fore').css({
					'margin': '0px 0px 0px 47px'
				});
				
				$('#' + divId + ' .pb-bd').css({
					'padding-left': '65px'
				});
				
				$('.pb-ok').addClass('color_df3134');
				
			}, 'POST', 'JSON');
		}else{
			
			var content = '<div id="user_add_bonus">' + 
								'<div class="tip-box icon-box">' +
									'<span class="' +divClass+ ' m-icon"></span>' + 
									'<div class="item-fore">' +
										'<h3 class="ftx-04">' + message + '</h3>' +
									'</div>' +
								'</div>' +
							'</div>';
			
			pb({
				id:divId,
				title:title,
				width:width,
				height:height,
				ok_title:ok_title,  //按钮名称
				cl_title:cl_title,  //按钮名称
				content:content,    //调取内容
				drag:false,
				foot:foot
			});
			
			$('#' + divId + ' .ftx-04').css({'padding': '11px 0px 0px 10px'});
			$('#' + divId + ' .tip-box').css({
				'width': '330px',
				'height': '50px',
				'padding': '0px 0px 10px 0px'
			});
			
			$('#' + divId + ' .item-fore').css({
				'margin': '0px 0px 0px 47px'
			});
			
			$('#' + divId + ' .pb-bd').css({
				'padding-left': '65px'
			});
		}

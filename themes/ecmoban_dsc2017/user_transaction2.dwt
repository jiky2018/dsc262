                    }
                    else {
                        $$('children_' + rec_id).innerHTML = '';
                    }
                }
                
                function change_cause_response(res) {
                    rec_id = res.rec_id;
                    $$('children_' + rec_id).innerHTML = res.option;
                }
                
                function return_type_order(){
                    var tval = $("input[name='return_type']:checked").val();    
                    var rec_id = $("input[name='return_rec_id']").val();            
                    if(tval == 0){
                        $('#maintain_'+rec_id).parent('.return_div').removeClass("hide").siblings().addClass("hide");
                        //$('.return_sub').hide();
                    }else if(tval == 1){
                        $('#returnid_'+rec_id).parent('.return_div').removeClass("hide").siblings().addClass("hide");
                        //$('.return_sub').show();
                        //return_app(tval, rec_id);
                    }else if(tval == 2){
                        $('#changeid_'+rec_id).parent('.return_div').removeClass("hide").siblings().addClass("hide");
                        //$('.return_sub').show();
                        //return_app1(tval, rec_id);
                        get_spec();
                    }
                    
                    //数量为1
                    $("input[name='maintain_number']").val(1);
        
                    $("input[name='attr_num']").val(1);
                    $("input[name='return_num']").val(1);
                    
                    //维修退货换货数量为1
                    $("#maintain_span_" +rec_id).html(1);
                    $("#retired_" +rec_id).html(1);
                    $("#retired1_" +rec_id).html(1);
                }
                
                /**点击退货方法*/
                function  return_app(tval, rec_id) {
                    if (tval == 1) {
                        $('returnid_' + rec_id).style.display = "inline-block";
                        $$('retired_' + rec_id).innerHTML = 1;
                    }
                    else {
                        $$('returnid_' + rec_id).style.display = "none";
                    }
                }
                
                /**退货数量控制 **/
                function check_return_num(obj, num, rec_id, type) {
                    
                    if ($(obj).val() > num) {
                        $(obj).val(num);
                    }
                    if ($(obj).val() <= 0) {
                        $(obj).val(1);
                    }
                    var val = $(obj).val();
                    
                    if(type == 1){
                        $$('maintain_span_' + rec_id).innerHTML = val;
                    }else if(type == 2){
                        $$('retired_' + rec_id).innerHTML = val;
                    }
                }
                
                /**换货数量控制 **/
                function check_attr_num(obj, num, rec_id) {
                    var val = document.getElementById(obj);
                    if (val.value > num) {
                        val.value = num;
                    }
                    if (val.value <= 0) {
                        val.value = 1;
                    }
                    
                    $$('retired_' + rec_id).innerHTML = val.value;
                    $$('retired1_' + rec_id).innerHTML = val.value;
                }
                
                /**换货*/
                function return_app1(tval, rec_id) {
                if (tval == 2) {
                    $$('changeid_' + rec_id).style.display = "inline-block";
                    $$('splist_' +  rec_id).style.display = "block";
                }
                else {
                    $$('changeid_' + rec_id).style.display = "none";
                    $$('splist_' +  rec_id).style.display = "none";
                    $('#splist_' +  rec_id).innerTHML = '';
                    $$('back_div').style.display = "none";
                }
                }
                /**换货获得商品属性*/
                
                function get_spec() {
                    var rec_id = $("input[name='return_rec_id']").val();    
                    var g_id = $("input[name='return_g_id']").val();    
                    var g_number = $("input[name='return_g_number']").val();    
                    
                    Ajax.call('ajax_dialog.php?act=ajax_get_spec', 'rec_id=' + rec_id + '&g_id=' + g_id + '&g_number=' + g_number, get_pro_response, 'POST', 'JSON');
                }
                /**获得属性列表回调函数*/
                function get_pro_response(res) {
                document.getElementById('spec_' + res.rec_id).style.display = 'block';
                var spec = document.createElement('div');
                
                spec.innerHTML = res.spec;
                document.getElementById('spec_' + res.rec_id).innerHTML = '';
                document.getElementById('spec_' + res.rec_id).appendChild(spec);
                
                if(res.attr_val != ''){
                    var arr = new Array();
                    var attr_arr = new Array();
                    arr = res.attr_val.split(',');
                    for(var i=0; i<arr.length; i++){
                        attr_arr = arr[i].split('_');
                        $("#attr_" + attr_arr[0]).val(attr_arr[1]);
                    }
                }
                }
                /**选择商品属性*/
                
                function setChange(id, t, attr_id) {
                
                    for (var i = 0; i < t.parentNode.childNodes.length; i++) {
                        if (t.parentNode.childNodes[i].className == 'cattsel' || t.parentNode.childNodes[i].className == 'cattsel catcolor') {
                            t.parentNode.childNodes[i].className = 'catcolor';
                        }
                    }
                    t.className = "cattsel";
                    
                    $$('attr_' + attr_id).setAttribute("value", id);
                
                }

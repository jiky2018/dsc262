	<script>
	function new_addPC(){
		var pc = new Object;
		
		pc.pay_card_sn = $(".txt_input_cardnum2").val();
		pc.vid = {$vid};
		pc.password = $(".txt_input_cardpw2").val();
		pc.captcha = $(".captcha_error2").val();
		
		var foot = true;
		var ok_title, cl_title, divId, title, width, height, hrefCont;
		title = "提示";
		width = 445;
		height = 58;
		ok_title = "确定";
		cl_title = "取消";
		
		var divClass = 'warn-icon-wen';
		if(pc.value_card_sn == ''){
			message = '卡号不能为空';
			divClass = 'warn-icon';
			foot = false;
		}else if(pc.password == ''){
			message = '密码不能为空';
			divClass = 'warn-icon';
			foot = false;
		}else if(pc.captcha == ''){
			message = '验证码不能为空';
			divClass = 'warn-icon';

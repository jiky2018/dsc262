				<script type="text/javascript">
                /**提交验证表单*/
                function check_sub() {
                    var frm = $('#formReturn');
                    var rec_id = $("input[name='return_rec_id']").val();    
                    
                    if($("#cause_" + rec_id).val() == 0){
                        alert(return_one);
                        return false;
                    }else{
                        if($("#last_option_" + rec_id).val() == 0){
                            alert(return_two);
                            return false;
                        }
                    }
                    
                    if($(".text_desc").val() == ''){
                        alert(return_three);
                        return false;
                    }
                    
                    var selCountries = $('#selCountries_0').val();
                    var selProvinces = $('#selProvinces_0').val();
                    var selCities = $('#selCities_0').val();
                    
                    if(selCountries == 0){
                        alert(return_four);
                        return false;
                    }else if(selProvinces == 0){
                        alert(selProvinces);
                        return false;
                    }else if(selCities == 0){
                        alert(selCities);
                        return false;
                    }else{
                        var selDistricts = $('#selDistricts_0');
                        if(selDistricts.attr('style') != 'display:none'){
                            if(selDistricts.val() == 0){
                                alert(selDistricts);
                                return false;
                            }
                        }
                    }
                    
                    if($("input[name='return_address']").val() == ''){
                        alert(address_empty);
                        return false;
                    }
                    
                    if($("input[name='addressee']").val() == ''){
                        alert(Contact_name_empty);
                        return false;
                    }
                    
                    if($("input[name='mobile']").val() != ''){
                        if($("input[name='mobile']").val().length != 11){
                            alert(phone_format_error);
                            return false;
                        }
                    }else{
                        alert(msg_phone_blank);
                        return false;
                    }
                    
                    frm.action = frm.action + '?act=submit_return';
                    frm.submit();
                    return true;
                }
                
                $(document).ready(function(e) {
                    $("input[name='return_type']").each(function(index, element) {

            {foreach from=$page_total2 item=vo}
            <li {if $page==$vo }class="current"{/if}><a href=coupons.php?{$page_url}&p={$vo}>{$vo}</a></li>
            {/foreach}
            <li class="nextious"><a href="coupons.php?{$page_url}&p={$next_page}">{$lang.page_next}<i>&gt;&gt;</i></a></li>
        </ul>
    </div>
    {/if}

$(".bind-op-btn a").click(function(){
    var t = $(this).attr("t");
    var text = '';
    if(t == "unbind"){
        text = json_languages.Unbundling;
    }else if(t == "bind"){
        text = json_languages.binding;
    }
    var bind_cont = $(this).parents(".bind-list").find(".bind-cont");
    if(bind_cont.css('display') == 'block'){
        bind_cont.hide();
        $(this).html(text);
    }else{
        $(this).html(json_languages.stop);
        bind_cont.show();
    }
});

			filters: {
				max_file_size: '2mb', //最大上传文件大小（格式100b, 10kb, 10mb, 1gb）
				mime_types: [//允许文件上传类型
					{title: "files", extensions: "bmp,gif,jpg,png,jpeg"}
				]
			},
			multi_selection: true, //true:ctrl多文件上传, false 单文件上传
			init: {
				FilesAdded: function(up, files) { //文件上传前
					var len = $(".img-list-li li").length;
					plupload.each(files, function(file) { //遍历文件
							len ++;
					});
					if(len > 10){
						alert("最多只能上传10张图片");
					}else{
						//开始上传  单张循环上传
						submitBtn();
					}
				},
				FileUploaded: function(up, file, info) { //文件上传成功的时候触发
					var data = eval("(" + info.response + ")");
					if(data.error == 2){
						alert("登陆失效，请重新登陆");
						return;
					}else{
						$(".mslist").html(data.content);
						$(".return_images").show();
						mscoll();
					}
				},
				UploadComplete:function(up,file){
					//所有文件上传成功时触发	
				},
				Error: function(up, err) {
					//上传出错的时候触发
					alert(err.message);
				}
			}
		});
		uploader_gallery.init();    
		
		function submitBtn(){
			uploader_gallery.setOption("multipart_params");//设置传参
			uploader_gallery.start();//开始控件
		};
		
        /**提交验证表单*/
        function check_sub() {
            var frm = $('#formReturn');
            var rec_id = $("input[name='return_rec_id']").val();    
            
            if($("#cause_" + rec_id).val() == 0){
                alert(json_languages.return_one);
                return false;
            }else{
                if($("#last_option_" + rec_id).val() == 0){
                    alert(json_languages.return_two);
                    return false;
                }
            }
            
            if($(".text_desc").val() == ''){
                alert(json_languages.return_three);
                return false;
            }
            
            var selCountries = $('#selCountries_0').val();
            var selProvinces = $('#selProvinces_0').val();
            var selCities = $('#selCities_0').val();
            
            if(selCountries == 0){
                alert(json_languages.return_four);
                return false;
            }else if(selProvinces == 0){
                alert(json_languages.selProvinces);
                return false;
            }else if(selCities == 0){
                alert(json_languages.selCities);
                return false;
            }else{
                var selDistricts = $('#selDistricts_0');
                if(selDistricts.attr('style') != 'display:none'){
                    if(selDistricts.val() == 0){
                        alert(json_languages.selDistricts);
                        return false;
                    }
                }
            }
            
            if($("input[name='return_address']").val() == ''){
                alert(json_languages.address_empty);
                return false;
            }
            
            if($("input[name='addressee']").val() == ''){
                alert(Contact_name_empty);
                return false;
            }
            
            if($("input[name='mobile']").val() != ''){
                if($("input[name='mobile']").val().length != 11){
                    alert(json_languages.phone_format_error);
                    return false;
                }
            }else{
                alert(json_languages.msg_phone_blank);
                return false;
            }
            
            frm.action = frm.action + '?act=submit_return';
            frm.submit();
            return true;
        }
        

                        <div class="p-price">
                        {if $goods.promote_price neq ''}
                            {$goods.promote_price}
                        {else}
                            {$goods.shop_price}
                        {/if}
                        </div>
                        <div class="p-num">{$lang.sale_amount}：{$goods.sales_volume}</div>
                        <div class="p-commit"><span class="p-nice"><i class="icon"></i><span class="nice-stars ftx-04">{$goods.cmt_count}</span></span></div>
                    </div>
                </li>
                {/foreach}
            </ul>
        </div>
    </div>

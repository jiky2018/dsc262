<div id="content" class="w1200">
    {if $shop_info}
    <div class="shopCon">
        <div class="shopBox">
            <div class="shopHeader">
                <a href="{$shop_info.store_url}" class="shopHeader-logo" target="_blank"><img src="{$basic_info.logo_thumb}" width="80" height="80" /></a>
                <div class="shopHeader-info">
                    <a href="{$shop_info.store_url}" target="_blank" class="sHi-title">{$shop_info.shop_name}</a>
                    <p class="dp">{$lang.Main_brand}：<span>
                    {foreach from=$brand_list item=brand name=nobrand}
                            {if !$smarty.foreach.nobrand.last}
                                {$brand.brand_name},
                            {else}
                                {$brand.brand_name}
                            {/if}
                        {/foreach}
                    </span></p>
                    <p>{$lang.seat_of}：{$address}</p>
                </div>
                <div class="shopHeader-dsr">
                    <dl>
                        <dt class="label">{$lang.score_detail}</dt>
                        <dd>{$lang.goods}<em class="ftx-08">{$merch_cmt.cmt.commentRank.zconments.score}</em></dd>
                        <dd>{$lang.service}<em class="ftx-08">{$merch_cmt.cmt.commentServer.zconments.score}</em></dd>
                        <dd>{$lang.Invalid}<em class="ftx-08">{$merch_cmt.cmt.commentDelivery.zconments.score}</em></dd>
                    </dl>
                    <dl>
                        <dt class="label">{$lang.Compared_industry}</dt>
                        <dd>{$lang.goods}<em class="ftx-01">{$merch_cmt.cmt.commentRank.zconments.goodReview}%</em></dd>
                        <dd>{$lang.service}<em class="ftx-01">{$merch_cmt.cmt.commentServer.zconments.goodReview}%</em></dd>
                        <dd>{$lang.Invalid}<em class="ftx-01">{$merch_cmt.cmt.commentDelivery.zconments.goodReview}%</em></dd>
                    </dl>
                </div>
                <div class="shopHeader-enter">
                    <a href="{$shop_info.store_url}" target="_blank" class="sHe-shop mt25">{$lang.enter_the_shop}<i>>></i></a>
                </div>
            </div>
        </div>

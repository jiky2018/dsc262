<script type="text/javascript">
$(function(){
    if(isNeeded('#j_idx_focus')){
        var obj = $('#j_idx_focus');
        $.fn.xTaber({
            content: obj,
			img:".idx-focus-img",
            tab: obj,
            auto: true,
            style: 'left',

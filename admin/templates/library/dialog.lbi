    /**
     * 新增一个筛选属性
     */
    function addFilterAttr(obj)
	{
		var obj = $(obj);
		var parent = obj.parents("*[ectype='item']");
		var clone = parent.clone();
		clone.find("[ectype='operation']")
		.attr("onclick",'removeFilterAttr(this)')
		.html("[-]");

		parent.after(clone);
	}

    
    /**
     * 删除一个筛选属性
     */
    function removeFilterAttr(obj)
	{
		var obj = $(obj);
		var parent = obj.parents("*[ectype='item']");
		parent.remove();

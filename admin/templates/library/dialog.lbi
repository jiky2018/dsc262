               location.href = 'category.php?act=title_remove&dt_id=' + dt_id + '&cat_id=' + {$cat_id};  
               {/if}
           }
          }else{
              var row = rowindex(obj.parentNode.parentNode);
              var tbl = document.getElementById('documentTitle_table');
        
              tbl.deleteRow(row);
          }
      }

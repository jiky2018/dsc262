<!doctype html>
<html>
<head>{include file ='library/admin_html_head.lbi'}</head>

<body class="iframe_body">
	<div class="warpper">
    	<div class="title"><a href="{$action_link.href}" class="s-back">{$lang.back}</a>{$lang.08_members} - {$ur_here}</div>
        <div class="content">
        	<div class="explanation" id="explanation">
            	<div class="ex_tit"><i class="sc_icon"></i><h4>{$lang.operating_hints}</h4><span id="explanationZoom" title="{$lang.fold_tips}"></span></div>
                <ul>
                	<li></li>
                </ul>
            </div>
            <div class="flexilist">
                <div class="common-content">
                    <div class="mian-info">
						<form action="user_vat.php" id="myForm" method="post" name="theForm" enctype="multipart/form-data">
                            <div class="switch_info">
                                 <div class="items">
                                    <div class="item">
                                        <div class="label">{$lang.company_name}：</div>
                                        <div class="label_value">
                                        	<input type="text" name="name" id="name" class="text" readonly="readonly" value="{$vat_info.company_name}" autocomplete="off" />
                                        </div>
                                    </div>
									<div class="item">
                                        <div class="label">{$lang.tax_id}：</div>
                                        <div class="label_value">
                                        	<input type="text" name="name" id="name" class="text" readonly="readonly" value="{$vat_info.tax_id}" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="item"> 
                                        <div class="label">{$lang.company_address}：</div> 
                                        <div class="label_value"> 
                                        	<input type="text" name="name" id="name" class="text" readonly="readonly" value="{$vat_info.company_address}" autocomplete="off" /> 
                                        </div> 
                                    </div> 
									<div class="item">
                                        <div class="label">{$lang.company_telephone}：</div>
                                        <div class="label_value">
                                        	<input type="text" name="name" id="name" class="text" readonly="readonly" value="{$vat_info.company_telephone}" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="item">
                                        <div class="label">{$lang.bank_of_deposit}：</div>
                                        <div class="label_value">
                                        	<input type="text" name="name" id="name" class="text" readonly="readonly" value="{$vat_info.bank_of_deposit}" autocomplete="off" />
                                        </div>
                                    </div>
									<div class="item">
                                        <div class="label">{$lang.bank_account}：</div>
                                        <div class="label_value">
                                        	<input type="text" name="name" id="name" class="text" readonly="readonly" value="{$vat_info.bank_account}" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="item">
                                        <div class="label">{$lang.consignee_name}：</div>
                                        <div class="label_value">
                                        	<input type="text" name="name" id="name" class="text" readonly="readonly" value="{$vat_info.consignee_name}" autocomplete="off" />
                                        </div>
                                    </div>
									<div class="item">
                                        <div class="label">{$lang.consignee_mobile_phone}：</div>
                                        <div class="label_value">
                                        	<input type="text" name="name" id="name" class="text" readonly="readonly" value="{$vat_info.consignee_mobile_phone}" autocomplete="off" />
                                        </div>
                                    </div>
                                    <div class="item">
                                        <div class="label">{$lang.consignee_province}：</div>
                                        <div class="label_value">
                                        	<input type="text" name="name" id="name" class="text" readonly="readonly" value="{$new_vat_consignee_list.region}" autocomplete="off" />
                                        </div>
                                    </div>
									<div class="item">
                                        <div class="label">{$lang.consignee_address}：</div>
                                        <div class="label_value">
                                        	<input type="text" name="name" id="name" class="text" readonly="readonly" value="{$vat_info.consignee_address}" autocomplete="off" />
                                        </div>
                                    </div>		
									<div class="item">
                                        <div class="label">{$lang.audit_status}：</div>
                                        <div class="label_value">
											<div class="checkbox_items">
                                                <div class="checkbox_item">
                                                    <input type="radio" class="ui-radio" name="audit_status" value="0" id="audit_status_1" {if $vat_info.audit_status eq 0}checked="checked"{/if} />
                                                    <label class="ui-radio-label" for="audit_status_1">{$lang.status_0}</label>
                                                </div>
                                                <div class="checkbox_item">
                                                    <input type="radio" class="ui-radio" name="audit_status" value="1" id="audit_status_2" {if $vat_info.audit_status eq 1}checked="checked"{/if}/>
                                                    <label class="ui-radio-label" for="audit_status_2">{$lang.status_1}</label>
                                                </div>
                                                <div class="checkbox_item">
                                                    <input type="radio" class="ui-radio" name="audit_status" value="2" id="audit_status_3" {if $vat_info.audit_status eq 2}checked="checked"{/if}/>
                                                    <label class="ui-radio-label" for="audit_status_3">{$lang.status_2}</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>								
                                    <div class="item">
                                        <div class="label">&nbsp;</div>
                                        <div class="label_value info_btn">
											<input type="submit" value="{$lang.button_submit}" class="button" id="submitBtn" />
											<input type="hidden" name="act" value="{$form_act}" />
											<input type="hidden" name="id" value="{$vat_info.id}" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
		</div>
	</div>
    {include file ='library/pagefooter.lbi'}
</body>
</html>

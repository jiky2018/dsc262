<!doctype html>
<html>
<head>{include file ='library/admin_html_head.lbi'}</head>

<body class="iframe_body">
	<div class="warpper">
    	<div class="title"><a href="{$action_link.href}" class="s-back">{$lang.back}</a>{$lang.goods_alt} - {$ur_here}</div>
        <div class="content">
        	<div class="explanation" id="explanation">
            	<div class="ex_tit"><i class="sc_icon"></i><h4>{$lang.operating_hints}</h4><span id="explanationZoom" title="{$lang.fold_tips}"></span></div>
                <ul>
                	<li>{$lang.operation_prompt_content.import.0}</li>
                    <li>{$lang.operation_prompt_content.import.1}</li>
                </ul>
            </div>   
            <div class="flexilist">
                <div class="common-content">
                    <div class="mian-info">
						<div class="list-div">{$lang.current_modification_data} {$record_count|default:0} {$lang.tiao}</div>
						<div style=" width:100px; height:10px; clear:both; overflow:hidden;"></div>
						<div class="list-div">
						<table id="listTable">
							<tr>
                            	<th width="10%"><div class="tDiv">{$lang.record_id}<div></th>
								<th width="10%"><div class="tDiv">{$lang.goods_id}</div></th>
								<th width="15%"><div class="tDiv">{$lang.goods_name}</div></th>
								<th width="15%"><div class="tDiv">{$lang.import_state}</div></th>
							</tr>
						</table>
						</div>
                    </div>
                </div>
            </div>
		</div>
	</div>
	{include file ='library/pagefooter.lbi'}
    <script type="text/javascript">
	$(function(){
		start({$page});
		ajax_title();
	});
	var user_id = {$user_id};
	function start(page)
	{
		Ajax.call('goods_lib.php?act=import_action_list', 'page=' + page + '&user_id=' + user_id, start_response, 'POST', 'JSON');
	}
	
	/**
	 * 处理反馈信息
	 * @param: result
	 * @return
	 */
	function start_response(result)
	{
		if(result.is_stop == 1){
			var tbl = document.getElementById("listTable"); //获取表格对象
			var row = tbl.insertRow(-1);
			
			cell = row.insertCell(0);
			cell.innerHTML = "<div class='tDiv'>"+result.filter_page+"</div>";
			cell = row.insertCell(1);
			cell.innerHTML = "<div class='tDiv'>"+result.list.goods_id+"</div>";
			cell = row.insertCell(2);
			cell.innerHTML = "<div class='tDiv'>"+result.list.goods_name+"</div>";
			cell = row.insertCell(3);
			cell.innerHTML = "<div class='tDiv'>"+result.list.status+"</div>";
			
			if(result.is_stop == 1){
				start(result.page);
			}	
		}
		
		if(result.is_stop == 0){
			$("#title_name").addClass("red");
			$("#title_name").html(goods_lib_title_name_one);
		}else{
			$("#title_name").html(goods_lib_title_name_two);
		}
	}
    </script>
</body>
</html>